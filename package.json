{
  "name": "copy-of-ai-증명사진-전문가-by-text",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "//--- 개발용 (Development) ---//": "---",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "vite frontend",
    "dev:backend": "tsx --watch backend/server.ts",
    "//--- 빌드용 (Build) ---//": "---",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "vite build frontend",
    "build:backend": "tsc -p ./backend/tsconfig.json",
    "//--- 프로덕션 실행용 (Production Start) ---//": "---",
    "start": "node backend/dist/server.js"
  },
  "dependencies": {
    "@google/genai": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
